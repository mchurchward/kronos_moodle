<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/elisreports/db" VERSION="20110330" COMMENT="XMLDB file for Moodle local/elisreports"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_elisreports_schedule" COMMENT="Scheduled reports" NEXT="local_elisreports_links">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="userid"/>
        <FIELD NAME="userid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="report owner" PREVIOUS="id" NEXT="report"/>
        <FIELD NAME="report" TYPE="char" LENGTH="63" NOTNULL="true" SEQUENCE="false" COMMENT="The report type" PREVIOUS="userid" NEXT="config"/>
        <FIELD NAME="config" TYPE="text" LENGTH="medium" NOTNULL="true" SEQUENCE="false" COMMENT="Serialized configuration for the scheduled task" PREVIOUS="report"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="userid_idx" UNIQUE="false" FIELDS="userid" NEXT="report_idx"/>
        <INDEX NAME="report_idx" UNIQUE="false" FIELDS="report" PREVIOUS="userid_idx"/>
      </INDEXES>
    </TABLE>
    <TABLE NAME="local_elisreports_links" COMMENT="Scheduled reports attachment links" PREVIOUS="local_elisreports_schedule">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="scheduleid"/>
        <FIELD NAME="scheduleid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="schedule report id" PREVIOUS="id" NEXT="downloads"/>
        <FIELD NAME="downloads" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" COMMENT="number of times downloaded" PREVIOUS="scheduleid" NEXT="link"/>
        <FIELD NAME="link" TYPE="text" LENGTH="small" NOTNULL="true" SEQUENCE="false" COMMENT="link to report attachment" PREVIOUS="downloads" NEXT="exportformat"/>
        <FIELD NAME="exportformat" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" COMMENT="schedule report format" PREVIOUS="link" NEXT="timecreated"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="exportformat"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
